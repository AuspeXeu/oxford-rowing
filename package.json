{
  "name": "bumps",
  "homepage": "https://bumps.live",
  "description": "Oxford bumps",
  "version": "1.0.5",
  "author": {
    "name": "Christian Vaas",
    "email": "christianvaas@auspex.eu",
    "url": "https://auspex.eu"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/AuspeXeu/oxford-rowing.git"
  },
  "bugs": {
    "url": "https://github.com/AuspeXeu/oxford-rowing"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run deps && npm run ncu"
    }
  },
  "scripts": {
    "api": "node server.js",
    "frontend": "vue-cli-service serve",
    "dev": "concurrently --kill-others \"npm run api\" \"npm run frontend\"",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "server": "node server.js",
    "deps": "depcheck . --ignores=\"husky,sqlite3,@vue/cli-plugin-babel,@vue/cli-plugin-eslint,@vue/cli-service,concurrently,vue,babel-eslint,eslint,eslint-plugin-vue,vue-template-compiler\"",
    "ncu": "ncu"
  },
  "dependencies": {
    "axios": "0.19.0",
    "body-parser": "1.19.0",
    "cheerio": "1.0.0-rc.3",
    "compression": "1.7.4",
    "decode-html": "2.0.0",
    "express": "4.17.1",
    "express-uws": "4.0.2",
    "moment": "2.24.0",
    "nconf": "0.10.0",
    "request": "2.88.0",
    "uuid": "3.3.2",
    "vue": "2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.8.0",
    "@vue/cli-plugin-eslint": "3.8.0",
    "@vue/cli-service": "3.8.4",
    "babel-eslint": "10.0.2",
    "concurrently": "4.1.1",
    "eslint": "6.0.1",
    "eslint-plugin-vue": "5.2.3",
    "husky": "2.6.0",
    "vue-template-compiler": "2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "globals": {
      "axios": true,
      "ReconnectingWebSocket": true,
      "Vuetify": true,
      "Vue": true,
      "VueRouter": true,
      "moment": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/require-v-for-key": 1,
      "vue/no-unused-vars": 1,
      "vue/no-side-effects-in-computed-properties": 0,
      "newline-per-chained-call": 0,
      "object-curly-newline": 0,
      "class-methods-use-this": 0,
      "no-underscore-dangle": 0,
      "import/no-unresolved": 0,
      "import/no-dynamic-require": 0,
      "no-plusplus": 0,
      "func-names": 0,
      "prefer-const": [
        "error",
        {
          "destructuring": "all"
        }
      ],
      "no-shadow": [
        "error",
        {
          "allow": [
            "_",
            "err",
            "resolve"
          ]
        }
      ],
      "no-eval": 0,
      "global-require": 0,
      "no-return-assign": 0,
      "comma-dangle": [
        "error",
        "never"
      ],
      "nonblock-statement-body-position": [
        "error",
        "below"
      ],
      "max-len": [
        "error",
        {
          "code": 3500,
          "tabWidth": 2
        }
      ],
      "semi": 0,
      "no-console": [
        "error",
        {
          "allow": [
            "log",
            "error"
          ]
        }
      ],
      "object-curly-spacing": 0,
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "_|job|Job|log"
        }
      ],
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "consistent-return": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "maintainers": [
    {
      "name": "Christian Vaas",
      "email": "chrisvaas@gmail.com"
    }
  ]
}
