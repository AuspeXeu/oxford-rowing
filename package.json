{
  "name": "bumps",
  "homepage": "https://bumps.live",
  "description": "Oxford bumps",
  "version": "1.0.3",
  "author": {
    "name": "Christian Vaas",
    "email": "christianvaas@auspex.eu",
    "url": "https://auspex.eu"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/AuspeXeu/oxford-rowing.git"
  },
  "bugs": {
    "url": "https://github.com/AuspeXeu/oxford-rowing"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run deps && npm run ncu && npm run lint"
    }
  },
  "scripts": {
    "api": "node server.js",
    "frontend": "vue-cli-service serve",
    "dev": "concurrently --kill-others \"npm run api\" \"npm run frontend\"",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "server": "node server.js",
    "deps": "depcheck . --ignores=\"husky,sqlite3,@vue/cli-plugin-babel,@vue/cli-plugin-eslint,@vue/cli-service,concurrently,vue,babel-eslint,eslint,eslint-plugin-vue,vue-template-compiler\"",
    "ncu": "ncu"
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "compression": "1.7.3",
    "express": "4.16.4 ",
    "express-uws": "4.0.2",
    "nconf": "0.10.0",
    "request": "2.88.0",
    "uuid": "3.3.2",
    "vue": "2.6.7"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.4.1",
    "@vue/cli-plugin-eslint": "3.4.1",
    "@vue/cli-service": "3.4.1",
    "babel-eslint": "10.0.1",
    "concurrently": "4.1.0",
    "eslint": "5.14.1",
    "eslint-plugin-vue": "5.2.2",
    "husky": "1.3.1",
    "vue-template-compiler": "2.6.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "globals": {
      "axios": true,
      "ReconnectingWebSocket": true,
      "Vuetify": true,
      "Vue": true,
      "VueRouter": true,
      "moment": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/require-v-for-key": 1,
      "no-unused-vars": 1,
      "vue/no-unused-vars": 1,
      "vue/no-side-effects-in-computed-properties": 0,
      "no-console": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "maintainers": [
    {
      "name": "Christian Vaas",
      "email": "chrisvaas@gmail.com"
    }
  ]
}
